#!/usr/bin/env bash
set -euo pipefail

BASE_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")/.." && pwd)"

source "$BASE_DIR/lib/geoip_core.sh"
source "$BASE_DIR/lib/geoip_lookup.sh"
source "$BASE_DIR/lib/geoip_http.sh"
source "$BASE_DIR/lib/geoip_reverse.sh"
source "$BASE_DIR/lib/geoip_nmap.sh"
source "$BASE_DIR/lib/geoip_abuseipdb.sh"
source "$BASE_DIR/lib/geoip_whois.sh"
source "$BASE_DIR/lib/geoip_dns.sh"
source "$BASE_DIR/lib/geoip_recon.sh"

main "$@"